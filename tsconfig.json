{
  "include": [
        "src/**/*.ts",
        "src/**/*.js"
    ],
    "exclude": [
        "node_modules"   
    ],
 "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
 "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist", 
    "strict": true,
    "noImplicitAny": false,
 }
}